@model IEnumerable<AESManagement.AESDataService.Question>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Question
        </th>
        <th>
            Answer
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.theQuestion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.theAnswer)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.questionId })
        </td>
    </tr>
}

</table>
